{
	"name": "Chrome Dashboard",
	"description": "Chrome yeni tab ekranını daha kullanışlı bir ekran ile değiştirir.",
	"version": "1.0",
	"manifest_version": 2,
	"author": "Cenk SARI",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"64": "icons/icon64.png",
		"128": "icons/icon128.png",
		"256": "icons/icon256.png"
	},
	"chrome_url_overrides": {
		"newtab": "dashboard.html"
	},
	"options_page": "options.html",
	"content_security_policy": "script-src 'self' https://query.yahooapis.com/v1/public/yql; object-src 'self'"
}